{
  "expo": {
    "name": "Casa Owner",
    "slug": "casa",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "casa-owner",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1B1464"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.casa.owner",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:casagroup.au"
      ],
      "buildNumber": "11"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1B1464"
      },
      "package": "com.casa.owner",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "casagroup.au",
              "pathPrefix": "/owner"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#0D7377"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "8e409e27-49c7-410f-935c-e0ec17ade389"
      },
      "router": {}
    },
    "updates": {
      "url": "https://u.expo.dev/8e409e27-49c7-410f-935c-e0ec17ade389"
    },
    "runtimeVersion": "0.1.0",
    "owner": "roberto171s-organization"
  }
}
