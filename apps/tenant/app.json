{
  "expo": {
    "name": "Casa Tenant",
    "slug": "casa-tenant",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "casa-tenant",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D7377"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "au.com.casaintelligence.tenant",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:www.casaapp.com.au"
      ],
      "buildNumber": "12"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0D7377"
      },
      "package": "com.casa.tenant",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "www.casaapp.com.au",
              "pathPrefix": "/tenant"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#0D7377"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "c021f78c-e826-47c3-b4e1-aa1ad9e6f8c9"
      },
      "router": {}
    },
    "updates": {
      "url": "https://u.expo.dev/c021f78c-e826-47c3-b4e1-aa1ad9e6f8c9"
    },
    "runtimeVersion": "0.1.0",
    "owner": "casa-tenant"
  }
}
